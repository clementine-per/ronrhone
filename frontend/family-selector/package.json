{
  "name": "ronrhone-family-selector",
  "version": "1.0.0",
  "description": "Ronrhone Family Selector",
  "scripts": {
    "build": "npm run cleanup && npm run copy-assets && elm make src/FamilySelector.elm --output=build/app.js --optimize && uglifyjs build/app.js --compress 'pure_funcs=\"F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9\",pure_getters,keep_fargs=false,unsafe_comps,unsafe' | uglifyjs --mangle --output build/app.min.js && mv build/app.min.js build/app.js",
    "copy-assets": "rimraf build && mkdir build && cp -r public/* build",
    "cleanup": "rimraf elm-stuff/",
    "debug": "elm-live src/FamilySelector.elm --port=3000 --dir=public/ -- --output=public/app.js --debug",
    "deploy": "npm run build && gh-pages --dist build/",
    "live": "elm-live src/FamilySelector.elm --port=3000 --dir=public/ -- --output=public/app.js",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clementine-per/ronrhone.git"
  },
  "bugs": {
    "url": "https://github.com/clementine-per/ronrhone/issues"
  },
  "homepage": "https://github.com/clementine-per/ronrhone/tree/develop/frontend#readme",
  "author": "RÃ©my Hubscher",
  "license": "ISC",
  "devDependencies": {
    "elm": "^0.19.1-5",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.5",
    "elm-live": "^4.0.2",
    "elm-test": "^0.19.1-revision6",
    "gh-pages": "^3.1.0",
    "rimraf": "^3.0.2",
    "uglify-js": "^3.12.7"
  }
}
